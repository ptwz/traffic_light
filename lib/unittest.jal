-- Description: set of procedures used to perform unittest
--

var volatile bit _ut_trash_bit
var volatile byte _ut_trash_byte
var volatile word _ut_trash_word
var volatile sword _ut_trash_sword
var volatile dword _ut_trash_dword
var volatile sdword _ut_trash_sdword

procedure test_bit(bit in t) is
	pragma inline
	_ut_trash_bit = t
end procedure

procedure test_byte(byte in t) is
	pragma inline
	_ut_trash_byte = t
end procedure

procedure test_word(word in t) is
	pragma inline
    _ut_trash_word = t
end procedure

procedure test_sword(sword in t) is
	pragma inline
	_ut_trash_sword = t
end procedure

procedure test_dword(dword in t) is
	pragma inline
    _ut_trash_dword = t
end procedure

procedure test_sdword(sdword in t) is
	pragma inline
	_ut_trash_sdword = t
end procedure
